  <DataSets>
    <DataSet Name="ds">
      <Query>
        <DataSourceName>dsource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CropYear">
            <Value>=Parameters!CropYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
district_name
,(select  mill_Name from Miller_Registration_2017 as MR where MR.Registration_ID=QI.Mill_Name) as miller_Name

,QI.Agreement_ID
,QI.LotNumber
--,IA.issue_date
,convert(varchar(10), IA.issue_date, 101) as issue_date

,DATEDIFF(day,IA.issue_date,date) AS DiffDate
,convert(varchar(10), date, 101) as CMR_Date
,convert(varchar(10), GETDATE(), 101) as server

 from CMR_QualityInspection as QI
 left join pds.districtsmp as MP on MP.district_code=QI.District
 inner join PaddyMilling_IssueAgainst_DO as IA on IA.district_code=QI.District and IA.issueCentre_code=QI.issueCentre_code and IA.Partyname=QI.Mill_Name and IA.Godown=QI.Godown_Code and IA.CropYear=QI.CropYear
where
paddy_markfed is null or  paddy_markfed='' 
and QI. CropYear=@CropYear
 group by district_name, QI.Mill_Name, QI.Agreement_ID,QI.LotNumber, IA.issue_date, QI.Date

order by district_name</CommandText>
      </Query>
